{% extends 'base.html.twig' %}
{% form_theme form ':Form:fields.html.twig' %}

{% block body %}
{% set doc_links = {
    'Users list':'admin_user',
    'Roles list':'admin_role',
    'CSV files list':'csvfiles',
    }
%}

<div class="col-sm-3 col-md-2 sidebar">

    {% block submenu %}
        <ul class="nav nav-sidebar">
            {% for key,link in doc_links %}

                    <li><a href="{{ path(link~'_index') }}">{{ key|trans }}</a></li>

            {% endfor %}                       
        </ul>
    {% endblock %}

</div>
    <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
    <h1>CsvFiles creation</h1>

    {{ form_start(form, {'attr': {'role':'form', 'class':'form-horizontal'}}) }}

        {{ form_widget(form) }}

    {{ form_end(form) }}

</div>
{% endblock %}
