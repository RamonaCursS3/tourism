{% extends 'base.html.twig' %}

{% block body %}
    <h1>TouristAttractionImage</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ touristAttractionImage.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ touristAttractionImage.name }}</td>
            </tr>
            <tr>
                <th>Path</th>
                <td>{{ touristAttractionImage.path }}</td>
            </tr>
            <tr>
                <th>Datcre</th>
                <td>{% if touristAttractionImage.datCre %}{{ touristAttractionImage.datCre|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Datupd</th>
                <td>{% if touristAttractionImage.datUpd %}{{ touristAttractionImage.datUpd|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('touristattractionimage_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('touristattractionimage_edit', { 'id': touristAttractionImage.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
